{
  "name": "micro-webcruiter-from-api",
  "description": "Generate webcruiter rss from api",
  "version": "1.0.2",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.8.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "rss",
    "recruitments",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/micro-webcruiter-from-api.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-webcruiter-from-api/issues"
  },
  "homepage": "https://github.com/telemark/micro-webcruiter-from-api",
  "now": {
    "alias": [
      "recruitments.t-fk.win"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.18.2",
    "axios": "0.16.0",
    "coveralls": "2.13.0",
    "nsp": "2.6.3",
    "nyc": "10.2.0",
    "standard": "10.0.0",
    "test-listen": "1.0.1"
  },
  "dependencies": {
    "marked": "0.3.6",
    "micro": "7.3.0",
    "rss": "1.2.2",
    "valid-url": "1.0.9"
  }

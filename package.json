{
  "name": "micro-webcruiter-from-api",
  "description": "Generate webcruiter rss from api",
  "version": "1.0.15",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "micro",
    "rss",
    "recruitments",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark-arkiv/micro-webcruiter-from-api.git"
  },
  "bugs": {
    "url": "https://github.com/telemark-arkiv/micro-webcruiter-from-api/issues"
  },
  "homepage": "https://github.com/telemark-arkiv/micro-webcruiter-from-api#readme",
  "now": {
    "alias": [
      "recruitments.t-fk.win"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.24.0",
    "axios": "0.17.1",
    "coveralls": "3.0.0",
    "nsp": "3.1.0",
    "nyc": "11.4.1",
    "standard": "10.0.3",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "marked": "0.3.12",
    "micro": "9.1.0",
    "rss": "1.2.2",
    "valid-url": "1.0.9"
  }
}
